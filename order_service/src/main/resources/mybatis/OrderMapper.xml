<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.order_service.mapper.OrderMapper">
    <select id="getOrderAll" resultType="Productorder">
        SELECT * FROM productorder
    </select>
    <select id="getOrderById" resultType="Productorder">
        SELECT * FROM productorder WHERE id = #{id}
    </select>
</mapper>